{
	"AppName": "xbmc-callback-daemon",
	"ArtifactsDest": "../xbmc-callback-daemon-jekyll",
  "BuildConstraints": "!plan9,!netbsd,!openbsd,!dragonfly,!solaris",
	"ConfigVersion": "0.9",
	"Tasks": [
		"interpolate-source",
		"go-fmt",
		"default",
		"deb-source"
	],
	"MainDirsExclude": "Godeps",
	"TaskSettings": {
		"deb": {
      "other-mapped-files": {
        "/etc/init.d/xbmc-callback-daemon": "debian/xbmc-callback-daemon.init",
        "/etc/init/xbmc-callback-daemon.conf": "debian/xbmc-callback-daemon.upstart",
        "/etc/default/xbmc-callback-daemon": "debian/xbmc-callback-daemon.default",
        "/usr/share/doc/xbmc-callback-daemon/xbmc-callback-daemon.example.json": "contrib/config.example.json"
      }
		}
	},
	"BuildSettings": {
		"LdFlags": "-s"
	},
	"Env": [
		"GOPATH={{.Env.PWD}}{{.PS}}Godeps{{.PS}}_workspace{{.PLS}}src{{.PLS}}{{.Env.GOPATH}}"
	]
}
